function generic<T extends number, U extends 1 | 2, V extends T | U, W>(p1: T, p2: U, p3: V, p4: W) {
    Number.isNaN(p1);
    Number.isNaN(p2);
    Number.isNaN(p3);
    isNaN(p4);

    Number.isFinite(p1);
